ScreenManager:
    id: manager
    Screen:
        name: "Main"
        canvas.before:
            Color:
                rgb:0,1,0
            Rectangle:
                size: self.size
        BoxLayout:
            orientation:"vertical"
            padding:100
            spacing:10
            Button:
                text:"Set Up"
                on_press: root.manager.current = "Graphics"
            Button:
                text:"Ejecutar"
                on_press: manager.current = 'Graphics'
            Button:
                text:"Ayuda" 
    Screen:
        name: "Graphics"
        canvas.before:
            Color:
                rgb:1,0,0
            Rectangle:
                size: self.size
        GridLayout:
            cols: 2
            BoxLayout:
                orientation: 'vertical'
                size_hint_x:None
                Button:
                    text: 'FCFS'
                    on_press: print('mira que loco')
                Button:
                    text: 'SSTF'
                    on_press: print manager.screens
                Button:
                    text: 'SCAN'
                    on_press: manager.current = 'Main'
                Button:
                    text: 'CSCAN'
                    on_press: manager.current = 'Main'
                Button:
                    text: 'LOOK'
                    on_press: manager.current = 'Main'
                Button:
                    text: 'CLOOK'
                    on_press: manager.current = 'Main'
                Button:
                    text: '<'
                    on_press: manager.current = 'Main'
            GridLayout:
                rows:2
                RelativeLayout:
                    canvas:
                        Line:
                            rectangle: 60,0,511,self.height-50
                        Color: 
                            rgba:0,1,0,1
                        Line:
                            points:0,0,100,100
                    size:550,550
                    Label:
                        text:"0"
                        size_hint:.1,.1
                        pos:50+0,450
                    Label:
                        text:"50"
                        size_hint:.1,.1
                        pos:50+50,450
                    Label:
                        text:"100"
                        size_hint:.1,.1
                        pos:50+100,450
                    Label:
                        text:"150"
                        size_hint:.1,.1
                        pos:50+150,450
                    Label:
                        text:"200"
                        size_hint:.1,.1
                        pos:50+200,450
                    Label:
                        text:"250"
                        size_hint:.1,.1
                        pos:50+250,450
                    Label:
                        text:"300"
                        size_hint:.1,.1
                        pos:50+300,450
                    Label:
                        text:"350"
                        pos:50+350,450
                        size_hint:.1,.1
                    Label:
                        text:"400"
                        size_hint:.1,.1
                        pos:50+400,450
                    Label:
                        text:"450"
                        size_hint:.1,.1
                        pos:50+450,450
                    Label:
                        text:"500"
                        pos:50+500,450
                        size_hint:.1,.1
                BoxLayout:
                    size_hint_y:None
                    padding:20       
                    Label:
                        text:"Metodo: SSTF"
                    Label:
                        text:"Movimientos: 230"
                    Label:
                        text:"Direccion: Izquierda"